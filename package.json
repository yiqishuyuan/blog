    "antd": "^5.27.1",
    "axios": "^1.10.0",
    "dayjs": "^1.11.13",
    "echarts": "^6.0.0",
    "echarts-gl": "^2.0.9",
    "framer-motion": "^12.23.0",
    "fs-extra": "^11.3.1",
    "react": "^19.1.0",
    "react-countup": "^6.5.3",
    "react-dom": "^19.1.0",
    "react-redux": "^9.2.0",
    "react-router": "^7.6.3",
