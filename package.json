{
  "name": "my-vue-app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "pageBump": "node scripts/index.js",
    "prepare": "husky install",
    "lint": "eslint 'src/**/*.{ts,tsx}' --fix",
